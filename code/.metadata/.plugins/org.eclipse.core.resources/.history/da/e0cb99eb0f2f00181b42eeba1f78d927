package com.whoiszxl.blockchain.model;

import java.util.List;

/**
 * 区块结构模型
 * @author whoiszxl
 *
 */
public class Block {

	/**
	 * 区块索引号
	 */
	private int index;
	
	/**
	 * 当前区块的hash值,区块唯一标识
	 */
	private String hash;
	
	/**
	 * 生成区块的时间戳
	 */
	private long timestamp;
	
	/**
	 * 当前区块的交易集合
	 */
	private List<Transaction> transactions;
	
	/**
	 * 工作量证明,计算正确hash值的次数
	 */
	private int nonce;
	
	/**
	 * 前一个区块的hash值
	 */
	private String previousHash;

	public Block() {
		super();
	}
	
	
}
