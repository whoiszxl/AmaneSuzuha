package com.whoiszxl.blockchain.model;

import java.util.List;

/**
 * 区块结构模型
 * @author whoiszxl
 *
 */
public class Block {

	/**
	 * 区块索引号
	 */
	private int index;
	
	/**
	 * 当前区块的hash值,区块唯一标识
	 */
	private String hash;
	
	/**
	 * 生成区块的时间戳
	 */
	private long timestamp;
	
	private List<Transaction> transactions;
}
