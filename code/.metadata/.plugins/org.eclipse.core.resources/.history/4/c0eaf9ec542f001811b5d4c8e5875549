package com.whoiszxl.blockchain.model;

/**
 * 交易输入
 * @author whoiszxl
 *
 */
public class TransactionInput {

	/**
	 * 前一次交易的id
	 */
	private String txId;
	
	/**
	 * 交易金额
	 */
	private int value;
	
	/**
	 * 交易签名
	 */
	private String signature;
	
	/**
	 * 交易发送方的钱包公钥
	 */
	private String publicKey;
}
